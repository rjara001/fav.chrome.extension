{
    "manifest_version": 3,
    "version": "0.21",
    "short_name": "SpeedLink",
    "name": "Speed Link",
    "description": "Lightning-fast URL access with custom shortcuts. Effortlessly switch environments, manage pipelines, and access Jira. Boost your development productivity. Install now!",
    "homepage_url": "https://www.randuslab.com/",
    "action": {},
    "permissions": [
        "storage",
        "activeTab"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "web_accessible_resources": [
        {
            "resources": [],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>",
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "jquery/jquery.min.js",
                "js/box.util.js",
                "js/store.js",
                "js/autocomplete.util.js",
                "js/move.js",
                "constants.js",
                "content.js",
                "index.js"
            ],
            "css": [],
            "all_frames": true
        }
    ],
    "sandbox": {
        "pages": []
    }
}